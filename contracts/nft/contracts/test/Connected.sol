pragma solidity 0.8.26;

import "../evm/UniversalNFT.sol";

contract Connected is UniversalNFT {
    constructor(
        address payable gatewayAddress,
        address owner,
        string memory name,
        string memory symbol,
        uint256 gasLimit
    )
        UniversalNFT(gatewayAddress, gasLimit)
        Ownable(owner)
        ERC721(name, symbol)
    {}
}
